<html class="no-js" lang="en">
<head>
    <title><%= @document.title %></title>
    <%- @getBlock("meta").toHTML() %>
    <%- @getBlock("styles").add(["/excelsior/css/excelsior.css"]).toHTML() %>
    <%- @getBlock("styles").toHTML() %>

    <!-- Custom (Project) Styles -->
    <% if @document.customStyles : %>
      <% for style in @document.customStyles : %>
        <%- @getBlock("styles").add(["/project-assets/css/" + style + ".css" ]).toHTML() %>
      <% end %>
    <% end %>

    <%- @getBlock("scripts").add(["/excelsior/js/vendor/custom.modernizr.js"],{defer:false,async:false}).toHTML() %>

</head>
<body class="off-canvas">
    <a href="#global-nav" class="skip-to">Skip to Navigation</a>
    <a href="#page" class="skip-to">Skip to Content</a>
    <div class="pull-down-container show-for-small">
        <a href="#" id="pull-down" class="pull-down show-for-small" data-active="active-gov-bar">
            <span class="accessibility-text">Open New York State Search</span>
            <span class="gripper">
                <span></span>
            </span>
        </a>
    </div>
    <div class="gov-bar">
        <div class="gov-wrap">
            <div class="state-links">
                <a href="http://www.ny.gov/" class="gov-link-1">New York State</a>
                <a href="http://www.nysegov.com/citGuide.cfm?superCat=102&amp;cat=449&amp;content=main" class="gov-link-2">State Agencies</a>
            </div>
            <a href="#" id="gov-link-3" class="gov-link-3">Search all of NY.gov</a>
            <form action="http://www.search.cio.ny.gov/search" method="get" target="_blank">
                <fieldset>
                    <legend>Legend</legend>
                    <label for="site-search-box">Search</label>
                    <div class="search-container">
                        <label for="state-search" class="hide-off-screen">Search All of NYS</label>
                        <input type="search" name="q">
                        <input type="submit" value="Search NY">
                    </div>
                    <input type="hidden" name="sort" value="date:D:L:d1">
                    <input type="hidden" name="output" value="xml_no_dtd">
                    <input type="hidden" name="ie" value="UTF-8">
                    <input type="hidden" name="oe" value="UTF-8">
                    <input type="hidden" name="client" value="default_frontend">
                    <input type="hidden" name="proxystylesheet" value="default_frontend">
                    <input type="hidden" name="site" value="default_collection">
                </fieldset>
            </form>
        </div>
    </div>
    <header class="site-bar">
        <div class="site-wrap">
            <div class="site-title">
                <!-- === Place Site Title Here === -->
                <a href="index.html" class="site-logo">Excelsior Web Framework</a>
            </div>
            <div class="site-functions">
                <ul class="mobile-functions">
                    <li><a href="#" id="mobile-site-search" class="mobile-icons mobile-search-button" data-active="active-site-search">Search Toggle</a></li>
                    <li><a href="#" id="mobile-site-menu" class="mobile-icons mobile-menu-button" data-active="active-site-menu">Menu Toggle</a></li>
                </ul>
                <!-- Link to show the menu -->
                <div class="site-search">
                    <form action="#">
                        <fieldset>
                            <legend>Legend</legend>
                            <label for="site-search-box">Search</label>
                            <div class="search-container">
                                <input type="search" id="site-search-box" class="site-search-box">
                                <div class="site-button">
                                    <input type="submit" class="site-search-button" value="Search Agency">
                                </div>
                            </div>
                        </fieldset>
                    </form>
                </div>
            </div>
        </div>
    </header>
    <nav class="global-nav" id="global-nav">
        <div class="nav-container">

            <!-- === Place Global Navigation Here === -->

            <%-@partial('template-navigation.html')%>

            <!-- === End Global Navigation === -->
        </div>
    </nav>
    <div class="page">
        <div class="content-section">
            <noscript>
                <div class="alert-box">
                    <p><strong>JavaScript is currently disabled in your web browser. </strong><br /> For full functionality of this site, it is necessary to enable JavaScript. Here are <a href="http://www.enable-javascript.com/" target="_blank"> instructions how to enable JavaScript</a></p>
                </div>
            </noscript>

            <%- @content %>

        </div>
        <footer role="contentinfo" class="footer">

            <!-- === Place Footer Content Here === -->
            <div class="row">
                <div class="large-12 columns">
                    <ul>
                        <li><h4>Excelsior Web Framework</h4></li>
                        <li><a href="http://nys-its.github.com/go-responsive/">See the framework in action</a></li>
                    </ul>
                </div>
            </div>

            <!-- === End Footer Content === -->
        </footer>
    </div>

    <!-- Partials that must appear outside the content area -->
    <%-@partial('foundation-reveal-modals.html.md')%>

    <%- @getBlock("scripts").add(["/excelsior/js/vendor/jquery.js"],{defer:false,async:false}).toHTML() %>
    <%- @getBlock("scripts").add(["/excelsior/js/vendor/zepto.js"],{defer:false,async:false}).toHTML() %>
    <%- @getBlock("scripts").add(["/excelsior/js/core/core.js"],{defer:false,async:false}).toHTML() %>
    <%- @getBlock("scripts").add(["/excelsior/js/core/off-canvas.js"],{defer:false,async:false}).toHTML() %>
    <%- @getBlock("scripts").add(["/excelsior/js/foundation/foundation.js"],{defer:false,async:false}).toHTML() %>

    <!-- Vendor Scripts -->
    <% if @document.vendorScripts : %>
      <% for item in @document.vendorScripts : %>
            <%- @getBlock("scripts").add(["/excelsior/js/" + item + ".js"],{defer:false,async:false}).toHTML() %>
      <% end %>
    <% end %>

    <!-- Page Scripts -->
    <% if @document.customScripts : %>
      <% for item in @document.customScripts : %>
        <%- @getBlock("scripts").add(["/project-assets/js/" + item + ".js" ],{defer:false,async:false}).toHTML() %>
      <% end %>
    <% end %>

    <script>$(document).foundation();</script>


    <%- @getBlock("scripts").toHTML() %>
</body>
</html>